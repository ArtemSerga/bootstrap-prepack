{
  "name": "06-react-smart",
  "description": "06  |  Test React component, includes all children",
  "component": {
    "destDirPath": "src/client/components/{groupName}",
    "fileExtension": "jsx",
    "script": "component.js"
  },
  "modules":[
    {
      "id": "initialState",
      "destDirPath": "src/client/store",
      "name": "initialState.js",
      "script": "initialState.js"
    },
    {
      "id": "actions",
      "destDirPath": "src/client/actions/{groupName}",
      "name": "{componentName}Actions.js",
      "script": "actions.js"
    },
    {
      "id": "actionsIndex",
      "destDirPath": "src/client/actions/",
      "name": "index.js",
      "script": "actionsIndex.js"
    },
    {
      "id": "reducersIndex",
      "destDirPath": "src/client/reducers/",
      "name": "index.js",
      "script": "reducersIndex.js"
    }
  ]
}
